#! /usr/bin/env python

"""
test DNN performance
"""

import argparse


def main():
    parser = argparse.ArgumentParser(description='test DNN performance')
    parser.add_argument('-net',
                        metavar='NetName',
                        required=True,
                        type=str,
                        help='neural network name')
    parser.add_argument('-stim',
                        metavar='StimuliInfo',
                        required=True,
                        type=str,
                        help='a csv file which contains stimuli information')
    parser.add_argument('-out',
                        metavar='Output',
                        required=True,
                        type=str,
                        help='an output directory')
    args = parser.parse_args()


if __name__ == '__main__':
    main()
